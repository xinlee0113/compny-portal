<!DOCTYPE html>
<html lang="zh-CN">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= title %> - <%= texts.site.title %></title>
    <meta name="description" content="<%= (texts.services && texts.services.automotiveDevelopment && texts.services.automotiveDevelopment.description) || '专业的车载应用开发服务' %>">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet">
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css" rel="stylesheet">
    <link href="/css/style.css" rel="stylesheet">
    <style>
        .hero-section {
            background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
            color: white;
            padding: 100px 0;
        }
        .capability-card {
            transition: transform 0.3s ease, box-shadow 0.3s ease;
            border: none;
            height: 100%;
        }
        .capability-card:hover {
            transform: translateY(-5px);
            box-shadow: 0 15px 35px rgba(0,0,0,0.1);
        }
        .process-step {
            position: relative;
        }
        .process-step::before {
            content: '';
            position: absolute;
            left: 30px;
            top: 80px;
            width: 2px;
            height: calc(100% - 80px);
            background: #dee2e6;
            z-index: 0;
        }
        .process-step:last-child::before {
            display: none;
        }
        .step-number {
            width: 60px;
            height: 60px;
            background: #667eea;
            color: white;
            border-radius: 50%;
            display: flex;
            align-items: center;
            justify-content: center;
            font-weight: bold;
            font-size: 1.5rem;
            position: relative;
            z-index: 1;
        }
        .pricing-card {
            transition: transform 0.3s ease;
        }
        .pricing-card:hover {
            transform: scale(1.05);
        }
        .case-study-card {
            background: linear-gradient(45deg, #f8f9fa, #ffffff);
            border-left: 4px solid #667eea;
        }
    </style>
</head>
<body>
    <!-- 导航栏 -->
    <%- include('../partials/navbar') %>

    <!-- 英雄区域 -->
    <section class="hero-section">
        <div class="container">
            <div class="row align-items-center">
                <div class="col-lg-8">
                    <h1 class="display-4 fw-bold mb-4"><%= serviceData.hero.title %></h1>
                    <p class="lead mb-4"><%= serviceData.hero.subtitle %></p>
                    <div class="row">
                        <% serviceData.hero.features.forEach(feature => { %>
                        <div class="col-md-3 col-6 mb-3">
                            <div class="d-flex align-items-center">
                                <i class="fas fa-check-circle text-warning me-2"></i>
                                <span><%= feature %></span>
                            </div>
                        </div>
                        <% }) %>
                    </div>
                    <div class="mt-4">
                        <a href="/contact" class="btn btn-warning btn-lg me-3">
                            <i class="fas fa-phone-alt me-2"></i>立即咨询
                        </a>
                        <a href="#capabilities" class="btn btn-outline-light btn-lg">
                            <i class="fas fa-arrow-down me-2"></i>了解更多
                        </a>
                    </div>
                </div>
                <div class="col-lg-4">
                    <div class="text-center">
                        <i class="fas fa-car text-warning" style="font-size: 8rem; opacity: 0.8;"></i>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <!-- 服务能力 -->
    <section id="capabilities" class="py-5">
        <div class="container">
            <div class="text-center mb-5">
                <h2 class="display-5 fw-bold">核心开发能力</h2>
                <p class="lead text-muted">专业的车载应用开发技术栈和解决方案</p>
            </div>
            
            <div class="row">
                <% serviceData.capabilities.forEach((capability, index) => { %>
                <div class="col-lg-4 mb-4" <% if(capability.id) { %>id="<%= capability.id %>"<% } %>>
                    <div class="card capability-card shadow-sm">
                        <div class="card-body p-4">
                            <div class="text-center mb-3">
                                <i class="<%= capability.icon %> text-primary" style="font-size: 3rem;"></i>
                            </div>
                            <h4 class="card-title text-center mb-3"><%= capability.title %></h4>
                            <p class="card-text text-muted mb-4"><%= capability.description %></p>
                            
                            <h6 class="fw-bold mb-2">技术栈：</h6>
                            <div class="mb-3">
                                <% capability.technologies.forEach(tech => { %>
                                <span class="badge bg-light text-dark me-1 mb-1"><%= tech %></span>
                                <% }) %>
                            </div>
                            
                            <h6 class="fw-bold mb-2">核心特性：</h6>
                            <ul class="list-unstyled">
                                <% capability.features.forEach(feature => { %>
                                <li class="mb-1">
                                    <i class="fas fa-check text-success me-2"></i><%= feature %>
                                </li>
                                <% }) %>
                            </ul>
                        </div>
                    </div>
                </div>
                <% }) %>
            </div>
        </div>
    </section>

    <!-- 开发流程 -->
    <section class="py-5 bg-light">
        <div class="container">
            <div class="text-center mb-5">
                <h2 class="display-5 fw-bold">开发流程</h2>
                <p class="lead text-muted">标准化的车载应用开发流程，确保项目成功交付</p>
            </div>

            <!-- V-Model 总览 -->
            <style>
                .vmodel-diagram{position:relative; max-width:1100px; margin:0 auto 2rem auto}
                .vmodel-columns{display:flex; justify-content:space-between; gap:24px}
                .vmodel-col{flex:1; display:flex; flex-direction:column; justify-content:space-between}
                .vmodel-box{background:#fff; border:1px solid #e9ecef; border-left:4px solid #0d6efd; border-radius:8px; padding:12px 14px; box-shadow:0 2px 10px rgba(0,0,0,.04)}
                .vmodel-center{display:flex; align-items:center; justify-content:center; margin:16px 0}
                .vmodel-code{font-size:.75rem; color:#6c757d}
                .vmodel-title{margin:0; font-weight:600}
                .vmodel-arrow{position:absolute; left:50%; top:0; bottom:0; width:2px; background:linear-gradient(to bottom right, transparent 48%, #dee2e6 50%, transparent 52%) repeat-y; background-size:22px 22px; transform:translateX(-50%) rotate(45deg); pointer-events:none}
                .vmodel-arrow-r{position:absolute; left:50%; top:0; bottom:0; width:2px; background:linear-gradient(to top right, transparent 48%, #dee2e6 50%, transparent 52%) repeat-y; background-size:22px 22px; transform:translateX(-50%) rotate(-45deg); pointer-events:none}
                @media (max-width: 992px){
                    .vmodel-columns{flex-direction:column}
                    .vmodel-arrow,.vmodel-arrow-r{display:none}
                }
            </style>

            <% const dp = serviceData.developmentProcess || []; %>
            <div class="vmodel-diagram position-relative">
                <% const leftIdx = [1,2,3,4,5]; const rightIdx = [11,10,9,8,7]; %>
                <style>
                    /* ASPICE V-Model标准布局：系统/软件工程过程组对称 */
                    .vmodel-grid{
                        max-width: 1200px;
                        margin: 0 auto;
                        position: relative;
                        padding: 40px 20px;
                    }
                    
                    /* 系统工程边界 */
                    .system-boundary {
                        border: 2px solid #28a745;
                        border-radius: 10px;
                        padding: 20px;
                        margin-bottom: 20px;
                        background: rgba(40, 167, 69, 0.05);
                        position: relative;
                    }
                    
                    .system-boundary::before {
                        content: "系统工程过程组";
                        position: absolute;
                        top: -12px;
                        left: 20px;
                        background: white;
                        padding: 0 10px;
                        color: #28a745;
                        font-weight: bold;
                        font-size: 12px;
                    }
                    
                    /* 软件工程边界 */
                    .software-boundary {
                        border: 2px solid #007bff;
                        border-radius: 10px;
                        padding: 20px;
                        background: rgba(0, 123, 255, 0.05);
                        position: relative;
                    }
                    
                    .software-boundary::before {
                        content: "软件工程过程组";
                        position: absolute;
                        top: -12px;
                        left: 20px;
                        background: white;
                        padding: 0 10px;
                        color: #007bff;
                        font-weight: bold;
                        font-size: 12px;
                    }
                    
                    /* 支持过程组边界 */
                    .support-boundary {
                        border: 2px solid #6c757d;
                        border-radius: 10px;
                        padding: 15px;
                        margin-bottom: 20px;
                        background: rgba(108, 117, 125, 0.05);
                        position: relative;
                    }
                    
                    .support-boundary::before {
                        content: "支持过程组";
                        position: absolute;
                        top: -12px;
                        left: 20px;
                        background: white;
                        padding: 0 10px;
                        color: #6c757d;
                        font-weight: bold;
                        font-size: 12px;
                    }
                    
                    /* 质量保证过程组边界 */
                    .quality-boundary {
                        border: 2px solid #ffc107;
                        border-radius: 10px;
                        padding: 20px;
                        margin-bottom: 20px;
                        background: rgba(255, 193, 7, 0.05);
                        position: relative;
                    }
                    
                    .quality-boundary::before {
                        content: "质量保证过程组";
                        position: absolute;
                        top: -12px;
                        left: 20px;
                        background: white;
                        padding: 0 10px;
                        color: #ffc107;
                        font-weight: bold;
                        font-size: 12px;
                    }
                    
                    /* 组织过程组边界 */
                    .organization-boundary {
                        border: 2px solid #6f42c1;
                        border-radius: 10px;
                        padding: 20px;
                        background: rgba(111, 66, 193, 0.05);
                        position: relative;
                    }
                    
                    .organization-boundary::before {
                        content: "组织过程组";
                        position: absolute;
                        top: -12px;
                        left: 20px;
                        background: white;
                        padding: 0 10px;
                        color: #6f42c1;
                        font-weight: bold;
                        font-size: 12px;
                    }
                    
                    /* 质量保证过程和组织过程的布局 */
                    .quality-processes, .org-processes {
                        display: flex;
                        justify-content: space-between;
                        gap: 10px;
                        flex-wrap: wrap;
                    }
                    
                    .quality-box, .org-box {
                        flex: 1;
                        min-width: 200px;
                        max-width: 250px;
                        background: white;
                        border: 1px solid #e9ecef;
                        border-radius: 6px;
                        padding: 12px;
                        text-align: center;
                    }
                    
                    /* 支持过程组行 */
                    .sup-row { margin-bottom: 15px; justify-content: center; }
                    
                    /* 质量和组织过程行 */
                    .quality-row, .org-row { margin-bottom: 15px; }
                    
                    /* V字形行布局 */
                    .v-row {
                        display: flex;
                        justify-content: space-between;
                        margin-bottom: 15px;
                        align-items: center;
                    }
                    
                    /* 系统级行 */
                    .sys-row { margin-bottom: 20px; }
                    
                    /* 软件级行 */
                    .swe-row { margin-bottom: 15px; }
                    
                    /* 左侧节点 */
                    .v-left { flex: 0 0 45%; }
                    /* 右侧节点 */
                    .v-right { flex: 0 0 45%; }
                    /* 底部并排节点 */
                    .v-bottom-pair { display: flex; justify-content: space-between; gap: 20px; }
                    /* 移除连接线，保持干净的V字形布局 */
                    
                    /* 每个节点增加视觉效果 */
                    .vmodel-box{
                        position:relative; z-index:2;
                        box-shadow: 0 3px 6px rgba(0,0,0,0.1);
                        border-radius: 6px;
                        transition: all 0.3s ease;
                        background: white;
                        border: 1px solid #e9ecef;
                        padding: 12px;
                        min-height: 80px;
                        display: flex;
                        flex-direction: column;
                        justify-content: center;
                    }
                    .vmodel-box:hover{
                        transform: translateY(-2px);
                        box-shadow: 0 6px 12px rgba(0,123,255,0.15);
                        border-color: #007bff;
                    }
                    .vmodel-code{
                        font-size: 11px;
                        color: #007bff;
                        font-weight: 600;
                        margin-bottom: 4px;
                    }
                    .vmodel-title{
                        font-size: 13px;
                        color: #333;
                        line-height: 1.3;
                        margin: 0;
                        font-weight: 500;
                    }
                    @media (max-width: 992px){
                        .vline-left,.vline-right{display:none}
                        .vmodel-grid{grid-template-columns:1fr}
                        .vmodel-grid .c{grid-column:auto}
                    }
                </style>
                <div class="vmodel-grid" aria-label="ASPICE V-Model">
                    <!-- 支持过程组 (SUP) -->
                    <div class="support-boundary">
                        <!-- SUP.1 项目策划与立项 -->
                        <div class="v-row sup-row">
                            <% if(dp[0]){ %>
                                <div class="vmodel-box v-center">
                                    <div class="vmodel-code">步骤 <%= dp[0].step %> · <%= (dp[0].title.match(/\((.*?)\)/) || [,''])[1] %></div>
                                    <p class="vmodel-title"><%= dp[0].title.replace(/\s*\(.*?\)\s*/,'') %></p>
                                </div>
                            <% } %>
                        </div>
                    </div>
                    
                    <!-- 系统工程过程组 (SYS) -->
                    <div class="system-boundary">
                        <!-- SYS.2 和 SYS.5 -->
                        <div class="v-row sys-row">
                            <% if(dp[1]){ %>
                                <div class="vmodel-box v-left">
                                    <div class="vmodel-code">步骤 <%= dp[1].step %> · <%= (dp[1].title.match(/\((.*?)\)/) || [,''])[1] %></div>
                                    <p class="vmodel-title"><%= dp[1].title.replace(/\s*\(.*?\)\s*/,'') %></p>
                                </div>
                            <% } %>
                            <% if(dp[11]){ %>
                                <div class="vmodel-box v-right">
                                    <div class="vmodel-code">步骤 <%= dp[11].step %> · <%= (dp[11].title.match(/\((.*?)\)/) || [,''])[1] %></div>
                                    <p class="vmodel-title"><%= dp[11].title.replace(/\s*\(.*?\)\s*/,'') %></p>
                                </div>
                            <% } %>
                        </div>
                        
                        <!-- SYS.3 和 SYS.4-Do -->
                        <div class="v-row sys-row">
                            <% if(dp[2]){ %>
                                <div class="vmodel-box v-left">
                                    <div class="vmodel-code">步骤 <%= dp[2].step %> · <%= (dp[2].title.match(/\((.*?)\)/) || [,''])[1] %></div>
                                    <p class="vmodel-title"><%= dp[2].title.replace(/\s*\(.*?\)\s*/,'') %></p>
                                </div>
                            <% } %>
                            <% if(dp[10]){ %>
                                <div class="vmodel-box v-right">
                                    <div class="vmodel-code">步骤 <%= dp[10].step %> · <%= (dp[10].title.match(/\((.*?)\)/) || [,''])[1] %></div>
                                    <p class="vmodel-title"><%= dp[10].title.replace(/\s*\(.*?\)\s*/,'') %></p>
                                </div>
                            <% } %>
                        </div>
                        
                        <!-- SYS.4-Plan 和 SWE.6 -->
                        <div class="v-row sys-row">
                            <% if(dp[3]){ %>
                                <div class="vmodel-box v-left">
                                    <div class="vmodel-code">步骤 <%= dp[3].step %> · <%= (dp[3].title.match(/\((.*?)\)/) || [,''])[1] %></div>
                                    <p class="vmodel-title"><%= dp[3].title.replace(/\s*\(.*?\)\s*/,'') %></p>
                                </div>
                            <% } %>
                            <% if(dp[9]){ %>
                                <div class="vmodel-box v-right">
                                    <div class="vmodel-code">步骤 <%= dp[9].step %> · <%= (dp[9].title.match(/\((.*?)\)/) || [,''])[1] %></div>
                                    <p class="vmodel-title"><%= dp[9].title.replace(/\s*\(.*?\)\s*/,'') %></p>
                                </div>
                            <% } %>
                        </div>
                    </div>
                    
                    <!-- 软件工程过程组 (SWE) -->
                    <div class="software-boundary">
                        <!-- SWE.1 和 SWE.5 -->
                        <div class="v-row swe-row">
                            <% if(dp[4]){ %>
                                <div class="vmodel-box v-left">
                                    <div class="vmodel-code">步骤 <%= dp[4].step %> · <%= (dp[4].title.match(/\((.*?)\)/) || [,''])[1] %></div>
                                    <p class="vmodel-title"><%= dp[4].title.replace(/\s*\(.*?\)\s*/,'') %></p>
                                </div>
                            <% } %>
                            <% if(dp[8]){ %>
                                <div class="vmodel-box v-right">
                                    <div class="vmodel-code">步骤 <%= dp[8].step %> · <%= (dp[8].title.match(/\((.*?)\)/) || [,''])[1] %></div>
                                    <p class="vmodel-title"><%= dp[8].title.replace(/\s*\(.*?\)\s*/,'') %></p>
                                </div>
                            <% } %>
                        </div>
                        
                        <!-- SWE.2 在左侧 -->
                        <div class="v-row swe-row">
                            <% if(dp[5]){ %>
                                <div class="vmodel-box v-left">
                                    <div class="vmodel-code">步骤 <%= dp[5].step %> · <%= (dp[5].title.match(/\((.*?)\)/) || [,''])[1] %></div>
                                    <p class="vmodel-title"><%= dp[5].title.replace(/\s*\(.*?\)\s*/,'') %></p>
                                </div>
                            <% } %>
                        </div>
                        
                        <!-- SWE.3 和 SWE.4 并排在底部 -->
                        <div class="v-row swe-row">
                            <div class="v-bottom-pair">
                                <% if(dp[6]){ %>
                                    <div class="vmodel-box">
                                        <div class="vmodel-code">步骤 <%= dp[6].step %> · <%= (dp[6].title.match(/\((.*?)\)/) || [,''])[1] %></div>
                                        <p class="vmodel-title"><%= dp[6].title.replace(/\s*\(.*?\)\s*/,'') %></p>
                                    </div>
                                <% } %>
                                <% if(dp[7]){ %>
                                    <div class="vmodel-box">
                                        <div class="vmodel-code">步骤 <%= dp[7].step %> · <%= (dp[7].title.match(/\((.*?)\)/) || [,''])[1] %></div>
                                        <p class="vmodel-title"><%= dp[7].title.replace(/\s*\(.*?\)\s*/,'') %></p>
                                    </div>
                                <% } %>
                            </div>
                        </div>
                    </div>
                    
                    <!-- 质量保证过程组 (SQA) -->
                    <div class="quality-boundary">
                        <div class="v-row quality-row">
                            <div class="quality-processes">
                                <div class="vmodel-box quality-box">
                                    <div class="vmodel-code">SQA.1 · 质量保证</div>
                                    <p class="vmodel-title">质量保证策略与实施</p>
                                </div>
                                <div class="vmodel-box quality-box">
                                    <div class="vmodel-code">SUP.8 · 配置管理</div>
                                    <p class="vmodel-title">配置项管理与版本控制</p>
                                </div>
                                <div class="vmodel-box quality-box">
                                    <div class="vmodel-code">SUP.9 · 变更管理</div>
                                    <p class="vmodel-title">变更请求与影响分析</p>
                                </div>
                                <div class="vmodel-box quality-box">
                                    <div class="vmodel-code">SUP.10 · 联合评审</div>
                                    <p class="vmodel-title">里程碑评审与决策</p>
                                </div>
                            </div>
                        </div>
                    </div>
                    
                    <!-- 组织过程组 (ORG) -->
                    <div class="organization-boundary">
                        <div class="v-row org-row">
                            <div class="org-processes">
                                <div class="vmodel-box org-box">
                                    <div class="vmodel-code">ORG.2 · 改进管理</div>
                                    <p class="vmodel-title">过程改进与度量</p>
                                </div>
                                <div class="vmodel-box org-box">
                                    <div class="vmodel-code">ORG.3 · 人力资源</div>
                                    <p class="vmodel-title">培训与能力建设</p>
                                </div>
                                <div class="vmodel-box org-box">
                                    <div class="vmodel-code">ORG.4 · 基础设施</div>
                                    <p class="vmodel-title">工具链与环境管理</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- 图例：V两侧对应关系说明 -->
                <div class="text-center mt-3 small text-muted v-legend">
                    <span class="badge bg-light text-dark me-2">左：定义/设计（向下）</span>
                    <span class="badge bg-light text-dark me-2">底：实现/编码（SWE.3）</span>
                    <span class="badge bg-light text-dark">右：验证/确认（向上）</span>
                    <div class="mt-2">
                        <span class="me-3">SYS.2 ↔ SYS.5</span>
                        <span class="me-3">SYS.3 ↔ SYS.4‑Do</span>
                        <span class="me-3">SYS.4‑Plan ↔ SWE.6</span>
                        <span class="me-3">SWE.1 ↔ SWE.5</span>
                        <span>SWE.2 ↔ SWE.4</span>
                    </div>
                    <div class="mt-1 text-muted">
                        <small>注：左臂定义和设计逐步具体化；右臂验证和确认逐步抽象化</small>
                    </div>
                </div>
            </div>

            <div class="text-center mb-4">
                <span class="badge bg-secondary me-2">左侧：定义/设计</span>
                <span class="badge bg-info text-dark me-2">底部：实现/编码</span>
                <span class="badge bg-success">右侧：集成/验证/确认</span>
            </div>
            
            <div class="row">
                <% serviceData.developmentProcess.forEach(process => { %>
                <div class="col-lg-12 mb-4">
                    <div class="process-step">
                        <div class="row align-items-center">
                            <div class="col-lg-1 text-center">
                                <div class="step-number">
                                    <%= process.step %>
                                </div>
                            </div>
                            <div class="col-lg-11">
                                <div class="card border-0 shadow-sm">
                                    <div class="card-body">
                                        <div class="row">
                                            <div class="col-md-8">
                                                <h5 class="card-title fw-bold"><%= process.title %></h5>
                                                <p class="card-text text-muted mb-0"><%= process.description %></p>
                                            </div>
                                            <div class="col-md-4">
                                                <h6 class="fw-bold mb-2">交付物：</h6>
                                                <% process.deliverables.forEach(deliverable => { %>
                                                <span class="badge bg-primary me-1 mb-1"><%= deliverable %></span>
                                                <% }) %>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <% }) %>
            </div>
        </div>
    </section>

    <!-- 我们能做 / 不做 -->
    <section class="py-5 bg-light">
        <div class="container">
            <div class="row g-4">
                <div class="col-lg-6">
                    <div class="card h-100 shadow-sm">
                        <div class="card-body">
